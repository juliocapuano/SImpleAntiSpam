{
    "name": "juliocapuano/simple-anti-spam",
    "type": "library",
    "description": "",
    "version": "1.0.0",
    "keywords": [
        "juliocapuano",
        "simple-anti-spam"
    ],
    "homepage": "https://github.com/juliocapuano/simple-anti-spam",
    "license": "MIT",
    "authors": [
        {
            "name": "Julio Capuano",
            "email": "juliocapuano@gmail.com",
            "homepage": "https://github.com/juliocapuano",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "juliocapuano\\SimpleAntiSpam\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "juliocapuano\\SimpleAntiSpam\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
